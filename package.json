  "scripts": {
    "docker-build": "docker build . -t personopplysninger",
    "docker-start": "docker run --name personopplysninger -p 8080:8080 -t -d personopplysninger",
    "docker-kill": "npm run docker-stop && npm run docker-rm",
    "docker-rm": "docker rm personopplysninger",
    "docker-stop": "docker stop personopplysninger",
    "clean": "rimraf ./build",
    "lint": "./node_modules/.bin/eslint ./",
    "server": "node ./server.js",
    "start-js": "API_URL=/mock-api/person-info.json node scripts/start.js",
    "start": "npm-run-all -p watch-css start-js",
    "watch-css": "less-watch-compiler --main-file=index.less src/less src/css",
    "build-css": "less-watch-compiler --run-once --main-file=index.less src/less src/css",
    "build-js": "node scripts/build.js",
    "build": "npm-run-all -p build-css build-js",
    "test": "node scripts/test.js --env=jsdom --coverage"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}",
      "!src/build/*.js"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "jest-fetch-mock": "^1.7.5"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
